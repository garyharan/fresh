<turbo-stream action="replace" target="recommended_profile">
  <template>
      <% if params[:group_id] %>
        <%= turbo_frame_tag "recommended_profile", src: recommendations_path(group_id: params[:group_id]) %>
      <% else %>
        <%= turbo_frame_tag "recommended_profile", src: recommendations_path %>
      <% end %>
  </template>
</turbo-stream>

