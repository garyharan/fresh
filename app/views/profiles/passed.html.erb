<div class="w-full">
  <div class="mx-auto" style="max-width: 1024px;">
    <div class="flex flex-row md:order-last mx-auto pb-2 text-center">
      <h1 class="font-bold text-3xl mt-8 mx-4">
        Passed profiles
      </h1>
    </div>
  </div>
</div>

<div class="w-full">
  <div class="mx-auto" style="max-width: 1024px;">
    <ul class="space-y-2">
      <% @profiles.each do |profile| %>
        <%= render profile %>
      <% end %>
    </ul>
  </div>
</div>
