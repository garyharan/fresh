<turbo-stream action="update" target="images">
  <template>
    <%= render 'images/images', images: @images %>
  </template>
</turbo-stream>

<turbo-stream action="update" target="images_form">
  <template>
    <%= render "images/form", image: @image %>
  </template>
</turbo-stream>

<turbo-stream action="update" target="onboarding_final_step">
  <template>
    <%= render 'onboarding/final_step_button', images: @images %>
  </template>
</turbo-stream>
